  "scripts": {
    "install": "npm run install-imagediff-@types && grunt pngicons && tsc",
    "install-imagediff-@types": "mkdir node_modules/@types/imagediff 2> /dev/null; cp kive/portal/js/imagediff.d.ts node_modules/@types/imagediff/index.d.ts",
    "watch-all": "npm run ts-watch & npm run sass-watch",
    "ts-watch": "tsc -w",
    "sass-watch": "node-sass --watch --recursive --output kive --source-map true --source-map-contents kive"
