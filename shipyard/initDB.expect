#!/usr/bin/expect -f

spawn ./manage.py syncdb --traceback

# "You have requested a flush..."
expect -re "Are you sure you want to do this" {
  send "yes\r\n"
}

# "You just installed Django's auth system..."
expect -re "Would you like to create one now" {
  send "yes\r\n"
}

# "Username (leave blank to use..."
expect -re "Username" {
  send "\r"
}

expect -re "Email address" {
  send "\r"
}

expect -re "Password" {
  send "\r"
}

expect -re "Password" {
  send "\r"
}

exit
